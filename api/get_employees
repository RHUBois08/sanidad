<?php

$permit_id = $_GET['id'];

$sql = "SELECT * FROM employees WHERE permit_id = $permit_id";
$result = $conn->query($sql);

$employees =;
if ($result->num_rows > 0) {
    while($row = $result->fetch_assoc()) {
        $employees= $row;
    }
}

echo json_encode(['success' => true, 'data' => $employees]);

$conn->close();
?>